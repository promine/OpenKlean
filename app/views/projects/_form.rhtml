<% form_for(@project) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
	<p>
    <%= f.label :board_id %><br />
    <%= f.collection_select :board_id, @boards, :id, :description %>
  </p>
  <p>
    <%= f.submit 'Save' %>
  </p>
<% end %>